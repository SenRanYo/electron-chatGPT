{
  "name": "electron-vue",
  "version": "1.0.0",
  "description": "electron-vue",
  "author": "SenRanYo <1342554547@qq.com>",
  "license": "MIT",
  "main": "dist/main/index.js",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite -d",
    "dev:debug:force": "vite -d --force",
    "build": "npm run build:pre && electron-builder -wml",
    "build:pre": "npm run format:fix && vue-tsc --noEmit && vite build",
    "build:dir": "npm run build:pre && electron-builder --dir",
    "build:mac": "npm run build:pre && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --mac",
    "build:linux": "npm run build:pre && electron-builder --linux",
    "build:win": "npm run build:pre && electron-builder --windows",
    "lint": "eslint --ext .js,.ts,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.ts,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "format": "prettier .",
    "format:fix": "prettier . --write"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "@vueuse/router": "^9.13.0",
    "dexie": "^3.2.3",
    "nanoid": "^4.0.1",
    "openai": "^3.2.1",
    "pinia": "^2.0.33",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.44.4",
    "@arco-themes/vue-chat-gpt": "^0.0.1",
    "@iconify-json/ep": "^1.1.10",
    "@mdi/font": "^7.2.96",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.10.0",
    "less": "^4.1.3",
    "sass": "^1.60.0",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.1",
    "vite-plugin-doubleshot": "^0.0.9",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.13.6",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.2.0"
  }
}
